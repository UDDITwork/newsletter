{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://YOUR_CLOUD_RUN_URL/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
